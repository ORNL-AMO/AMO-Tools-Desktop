{
  "compileOnSave": false,
  "exclude": [
  "./node_modules/billboard.js/src/ChartInternal/data/IData.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node10",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": [
      "node",
      "jasmine"
    ],
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "es2022",
      "dom"
    ],
    "paths": {
      "process-flow-lib": [
        "./src/process-flow-lib"
      ]
    }
  }
}
