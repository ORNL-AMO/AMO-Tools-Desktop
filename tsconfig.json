{
  "compileOnSave": false,
  "exclude": [
  "./node_modules/billboard.js/src/ChartInternal/data/IData.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": ["node"],
    "target": "es2022",
    "module": "esnext",
    "lib": [
      "es2019",
      "dom"
    ],
    "paths": {
      "process-flow-lib": ["./src/process-flow-lib"],
    }
  }
}
