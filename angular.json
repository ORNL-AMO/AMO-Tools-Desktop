{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "amo-tools-desktop": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "lodash",
              "regression",
              "xlsx",
              "exceljs",
              "pptxgenjs",
              "file-saver",
              "moment",
              "plotly.js-dist",
              "papaparse"
            ],
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "./node_modules/measur-tools-suite/bin/",
                "output": "."
              },
              {
                "glob": "**/*",
                "input": "./process-flow-diagram-component/dist/",
                "output": "."
              },
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/assets/styles/two-panel.css",
              "src/assets/styles/tabs.css",
              "src/assets/styles/utilities.css",
              "src/assets/styles/integrations.css",
              "src/assets/styles/graphs.css",
              "src/assets/styles/forms.css",
              "src/assets/styles/help.css",
              "src/assets/styles/ssmt-diagram.css",
              "src/assets/styles/toggle-switch.css",
              "src/assets/styles/assessment-banner.css",
              "src/assets/styles/directory-dashboard.css",
              "src/assets/styles/sidebar.css",
              "src/assets/styles/plotly-overrides.css",
              "src/assets/styles/responsive-calculators.css",
              "src/assets/styles/explore-opps.css",
              "src/assets/styles/pagination.css",
              "src/assets/styles/welcome-screen.css"
            ],
            "scripts": [
              "node_modules/plotly.js-dist/plotly.js"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production-web": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production-web.ts"
                }
              ]
            },
            "development-web": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development-web.ts"
                }
              ]
            },
            "desktop": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.desktop.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "amo-tools-desktop:build"
          },
          "configurations": {
            "production-web": {
              "buildTarget": "amo-tools-desktop:build:production-web"
            },
            "development-web": {
              "buildTarget": "amo-tools-desktop:build:development-web"
            },
            "development": {
              "buildTarget": "amo-tools-desktop:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "amo-tools-desktop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "karmaConfig": "karma.conf.js",
            "tsConfig": "src/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "scripts": [],
            "styles": [
              "src/styles.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/assets/styles/two-panel.css",
              "src/assets/styles/tabs.css",
              "src/assets/styles/utilities.css",
              "src/assets/styles/graphs.css",
              "src/assets/styles/forms.css",
              "src/assets/styles/assessment-banner.css",
              "src/assets/styles/directory-dashboard.css",
              "src/assets/styles/sidebar.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.webmanifest"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "inlineTemplate": false,
      "prefix": "app",
      "style": "css",
      "standalone": true,
      "type": "Component"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "analytics": "c7c821ee-29e2-4918-ab71-1e94bd66b1fb",
    "cache": {
      "enabled": false
    }
  }
}