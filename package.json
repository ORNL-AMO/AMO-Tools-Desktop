{
  "name": "amo-tools-desktop",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "description": "AMO-Tools-Desktop",
  "author": "Advanced Manufacturing Office",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "electron": "electron .",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "./node_modules/.bin/electron-rebuild"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "angular2-color-picker": "^1.3.1",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3": "4.5.0",
    "d3-ng2-service": "^1.6.0",
    "dotenv": "^4.0.0",
    "font-awesome": "^4.7.0",
    "ng2-bootstrap": "^1.3.1",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.6"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "AMO-Tools-Desktop",
    "iconUrl": "https://raw.githubusercontent.com/ORNL-AMO/AMO-Tools-Desktop/blob/develop/src/favicon.ico",
    "directories": {
      "buildResources": "../AMO-Tools-Desktop/src/",
      "output": "../output/"
    },
    "win": {
      "target": "squirrel"
    },
    "windowsSquirrel": {
      "msi": "true"
    },
    "linux": {
      "target": "tar.gz"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.0",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.52",
    "@types/moment": "^2.13.0",
    "@types/node": "~6.0.60",
    "chart.js": "^2.5.0",
    "codelyzer": "~2.0.0",
    "css-loader": "^0.26.2",
    "electron": "^1.4.15",
    "electron-builder": "^13.9.0",
    "electron-builder-squirrel-windows": "^13.6.1",
    "electron-packager": "",
    "electron-rebuild": "^1.5.7",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng2-charts": "^1.5.0",
    "ngx-popover": "0.0.16",
    "protractor": "~5.1.0",
    "style-loader": "^0.13.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.0"
  }
}
