{
  "name": "amo-tools-desktop",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "description": "AMO-Tools-Desktop",
  "author": "Advanced Manufacturing Office",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "electron": "electron .",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "pack": "build --dir",
    "dist": "build"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "d3": "4.5.0",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3-ng2-service": "^1.6.0",
    "dotenv": "^4.0.0",
    "font-awesome": "^4.7.0",
    "glyphicons-halflings": "^1.9.0",
    "ng2-bootstrap": "^1.3.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2",
    "electron-reload": "^1.1.0"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "AMO-Tools-Desktop",
    "iconUrl": "https://raw.githubusercontent.com/ORNL-AMO/AMO-Tools-Desktop/blob/develop/src/favicon.ico",
    "directories": {
      "buildResources": "../AMO-Tools-Desktop/src/",
      "output": "../output/"  
    },
    "win": {
      "target": "squirrel"
    },
    "windowsSquirrel": {
    "msi": "true"
    },
    "linux": {
      "target": "tar.gz"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.52",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "electron": "^1.4.15",
    "electron-builder": "^13.9.0",
    "electron-packager": "",
    "electron-reload": "^1.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3",
    "electron-builder-squirrel-windows": "^13.6.1"
  }
}
