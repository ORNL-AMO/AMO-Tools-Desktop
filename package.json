{
  "name": "amo-tools-desktop",
  "version": "0.7.0-beta",
  "main": "main.js",
  "license": "MIT",
  "description": "MEASUR",
  "author": "Advanced Manufacturing Office",
  "repository": "https://github.com/ORNL-AMO/AMO-Tools-Desktop.git",
  "angular-cli": {},
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --aot",
    "start": "ng serve",
    "build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build",
    "build-watch": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --watch",
    "build-prod": "ng build --aot",
    "build-prod-windows": "./scripts/ng.sh build --prod --aot",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json",
    "electron": "electron .",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "postinstall": "electron-builder install-app-deps",
    "e2e": "ng e2e",
    "pack": "build --dir",
    "dist": "build",
    "windows": "./node_modules/.bin/build -w --x64 --ia32",
    "linux": "./node_modules/.bin/build -l --x64",
    "mac": "./node_modules/.bin/build -m --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/compiler-cli": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/forms": "9.0.0",
    "@angular/localize": "^9.0.2",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/platform-server": "9.0.0",
    "@angular/router": "9.0.0",
    "@ng-bootstrap/ng-bootstrap": "6.0.0",
    "ajv": "^5.2.2",
    "amo-tools-suite": "0.7.0",
    "bootstrap": "4.3.1",
    "c3": "^0.4.18",
    "core-js": "2.5.7",
    "d3": "4.5.0",
    "electron-log": "3.0.1",
    "electron-updater": "4.0.6",
    "font-awesome": "^4.7.0",
    "ngx-electron": "2.1.1",
    "regression": "^2.0.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "^6.4.0",
    "save-svg-as-png": "1.2.0",
    "zone.js": "0.10.2"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "MEASUR",
    "directories": {
      "output": "../output/"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/win/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "installerHeaderIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "tar.gz"
      ],
      "icon": "src/assets/icons/png/",
      "executableName": "MEASUR",
      "maintainer": "Advanced Manufacturing Office",
      "artifactName": "${productName}-${version}.${ext}",
      "category": "Science"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "src/assets/icons/mac/icon.icns",
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.inherit.plist",
      "entitlementsInherit": "entitlements.mac.inherit.plist",
      "gatekeeperAssess": false
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.1",
    "@angular-devkit/core": "9.0.1",
    "@angular-devkit/schematics": "9.0.1",
    "@angular/cli": "9.0.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.100",
    "@types/node": "11.11.1",
    "angular-plotly.js": "^1.4.2",
    "chart.js": "^2.7.0",
    "codelyzer": "5.2.1",
    "electron": "4.0.8",
    "electron-builder": "20.43.0",
    "electron-download-tf": "^4.3.4",
    "electron-packager": "13.1.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "jquery": "3.5.0",
    "json2csv": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash.foreach": "^4.5.0",
    "lodash.keys": "^4.2.0",
    "moment": "^2.18.1",
    "ngx-bootstrap": "5.1.1",
    "ngx-clipboard": "12.2.1",
    "papaparse": "^5.1.0",
    "plotly.js": "^1.51.3",
    "popper.js": "1.15.0",
    "protractor": "^5.4.1",
    "ts-node": "8.0.3",
    "tslint": "^5.13.1",
    "typescript": "3.7.5"
  }
}
