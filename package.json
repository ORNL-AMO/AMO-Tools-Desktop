{
  "name": "amo-tools-desktop",
  "version": "0.3.3-beta",
  "main": "main.js",
  "license": "MIT",
  "description": "MEASUR",
  "author": "Advanced Manufacturing Office",
  "repository": "https://github.com/ORNL-AMO/AMO-Tools-Desktop.git",
  "angular-cli": {},
  "engines": {
    "node": "^8.11.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-aot": "ng build --aot",
    "build-watch": "ng build --watch",
    "build-prod": "ng build --aot",
    "build-prod-windows": "./scripts/ng.sh build --prod --aot",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json",
    "electron": "electron .",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps",
    "windows": "./node_modules/.bin/build -w --x64 --ia32",
    "linux": "./node_modules/.bin/build -l --x64",
    "mac": "./node_modules/.bin/build -m --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.5",
    "@angular/common": "5.2.5",
    "@angular/compiler": "6.0.4",
    "@angular/compiler-cli": "6.0.4",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/platform-server": "5.2.5",
    "@angular/router": "5.2.5",
    "ajv": "^5.2.2",
    "amo-tools-suite": "0.3.3",
    "bootstrap": "4.1.2",
    "c3": "^0.4.18",
    "core-js": "^2.4.1",
    "d3": "4.5.0",
    "dotenv": "^4.0.0",
    "electron-download-tf": "^4.3.4",
    "electron-log": "^1.3.0",
    "electron-reload": "^1.2.2",
    "electron-updater": "^2.18.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "ng-c3-export": "^0.1.8",
    "ngx-clipboard": "^11.1.3",
    "ngx-electron": "1.0.2",
    "node-sass": "^4.9.0",
    "popper.js": "^1.12.3",
    "regression": "^2.0.0",
    "rxjs": "5.5.2",
    "save-svg-as-png": "1.2.0",
    "ts-helpers": "^1.1.1",
    "typescript": "2.7.2",
    "zone.js": "^0.8.12"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "MEASUR",
    "directories": {
      "output": "../output/"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/win/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "installerHeaderIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb"
      ],
      "icon": "src/assets/icons/png/",
      "executableName": "MEASUR",
      "maintainer": "Advanced Manufacturing Office"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/assets/icons/mac/icon.icns"
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/core": "0.6.8",
    "@angular-devkit/schematics": "0.6.8",
    "@angular/cli": "6.0.8",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.28",
    "chart.js": "^2.7.0",
    "codelyzer": "~2.0.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.6",
    "electron-builder": "19.53.2",
    "electron-packager": "^9.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json2csv": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash.foreach": "^4.5.0",
    "lodash.keys": "^4.2.0",
    "moment": "^2.18.1",
    "ng2-toasty": "^4.0.2",
    "ngx-bootstrap": "^2.0.0-beta.7",
    "ngx-clipboard": "^11.1.3",
    "node-loader": "^0.6.0",
    "protractor": "^5.4.1",
    "style-loader": "^0.13.2",
    "ts-node": "^3.3.0",
    "tslint": "~4.4.2"
  }
}
